<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<ItemGroup>
		<_MySqlFiles Include="$(MSBuildThisFileDirectory)..\scripts\$(MSBuildProjectName)\**\*.*" />

		<ContentWithTargetPath Include="@(_MySqlFiles)">
			<TargetPath>DbLive.MSSQL\%(RecursiveDir)%(Filename)%(Extension)</TargetPath>
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</ContentWithTargetPath>
	</ItemGroup>
</Project>