{
  // can be used to configure project
  // can be overwritten in separate migration folder

  /* Options:
      deployment - full deployment will be wrapped withing transaction
      migration - each migration wrapped by transaction
      task - migration task wrapped by transaction
      none - no transactions introduced */
  "TransactionWrapLevel": "migration",

  /* Options:
      Chaos,
	    ReadCommitted,
	    RepeatableRead,
	    Serializable */
  "TransactionIsolationLevel": "serializable",

  /* Search pattern for the testing files. 
     Testing files can be executed in the end of deployment. */
  "TestFilePattern": "*test.sql"

  /* TODO: add deployment siquence. The ordered list of path can be provided to keep deployment in this order. */
}
